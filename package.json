{
  "name": "practice-create-react-app-pc",
  "version": "1.0.0",
  "description": "练习创建react项目脚手架，pc端",
  "main": "index.js",
  "engines": {
    "node": ">10.4.1",
    "npm": ">6.1.0"
  },
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "start": "webpack-dev-server --config config/webpack.dev.js",
    "dev": "better-npm-run dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "debug:dev": "webpack --config config/webpack.dev.js",
    "debug:prod": "webpack --config config/webpack.prod.js"
  },
  "betterScripts": {
    "dev": {
      "command": "node --exec babel-node bin/dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fandand/practice-create-react-app-pc.git"
  },
  "author": "fandand",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fandand/practice-create-react-app-pc/issues"
  },
  "homepage": "https://github.com/fandand/practice-create-react-app-pc#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "^0.1.1",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.17.2",
    "webpack-dev-server": "^3.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
